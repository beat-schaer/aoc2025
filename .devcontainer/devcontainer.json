{
  "name": "AOC2025 devcontainer",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "workspaceFolder": "/workspace/aoc2025",
  "workspaceMount": "source=${localWorkspaceFolder},target=/workspace/aoc2025,type=bind",
  "mounts": [
    "source=${localEnv:HOME}/.ssh,target=/home/builder/.ssh,type=bind,consistency=cached"
  ],
  "customizations": {
    "vscode": {
      "settings": {
        "terminal.integrated.shell.linux": null
      },
      "extensions": [
        "rust-lang.rust-analyzer",
        "vadimcn.vscode-lldb",
        "tamasfe.even-better-toml",
        "fill-labs.dependi",
        "mhutchie.git-graph",
        "asciidoctor.asciidoctor-vscode",
        "grimmer.vscode-back-forward-button"
      ]
    }
  },
  "remoteUser": "builder"
}
